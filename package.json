{
  "name": "@wix/wsr-playroom",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": {
    "name": "Shlomi Toussia-Cohen",
    "email": "shlomitc@wix.com"
  },
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/",
    "versionBumpStrategy": "minor"
  },
  "scripts": {
    "start": "yoshi start",
    "build": "yoshi build",
    "test": "yoshi test",
    "lint": "yoshi lint",
    "posttest": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "yoshi lint"
  },
  "devDependencies": {
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "yoshi": "^4.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "yoshi": {
    "hmr": "auto"
  },
  "babel": {
    "presets": [
      [
        "yoshi",
        {
          "ignoreReact": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "yoshi-base"
  }
}
